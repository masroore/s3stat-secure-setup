<UserControl x:Class="S3stat.SecureSetup.Content.EndpointDetailComplete"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
	<Grid>
		<StackPanel Margin="50,0,50,50">
			<TextBlock Text="Success" Style="{StaticResource Heading2}" />
			<TextBlock TextWrapping="WrapWithOverflow">
				Click on another item in the list to set it up with S3stat.
				<LineBreak/>
				<LineBreak/>
				Click the button below when everything is set up correctly.
				<LineBreak/>
				<Button x:Name="btnContinue" Content="Continue" Margin="00,16,0,0" HorizontalAlignment="Left" Height="50" Width="150" Click="btnContinue_Click" />
				<LineBreak/>
				<LineBreak/>
				<LineBreak/>
			</TextBlock>
		</StackPanel>
	</Grid>
</UserControl>
